      "source": [
        "def is_prime(num):\n",
        "  if num<=1:\n",
        "    return False\n",
        "  for i in range(2,int(num**0.5)+1):\n",
        "    if num% i==0:\n",
        "      return False\n",
        "  return True\n",
        "\n",
        "number=int(input(\"enter a number:\"))\n",
        "if is_prime(number):\n",
        "  print(\"the number is prime.\")\n",
        "else:\n",
        "  print(\"the number is not prime.\")"
      ]
